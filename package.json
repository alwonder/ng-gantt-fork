{
  "name": "ng-gantt",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "ng serve dev",
    "build": "npm run copy && ng build",
    "copy": "cp -R dist/ng-gantt src/ng-gantt",
    "build:app": "npm run copy && ng build --project packages && rimraf src/ng-gantt",
    "build:lib": "rimraf dist && ng build lib",
    "publish-npm": "npm run build:lib && cd dist/ng-gantt && npm publish",
    "reload": "npm run build:lib && npm run build && npm run start",
    "dev": "cd dist/app && http-server",
    "test": "ng test dev --watch=false && npm run e2e",
    "unittest": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "kill": "kill -9 $(lsof -t -i:4200)",
    "reinstall": "rm package-lock.json && rm -rf node_modules && npm i",
    "sass:rebuild": "npm rebuild node-sass"
  },
  "dependencies": {
    "@angular/common": "16.2.3",
    "@angular/compiler": "16.2.3",
    "@angular/core": "16.2.3",
    "@angular/forms": "16.2.3",
    "@angular/platform-browser": "16.2.3",
    "@angular/platform-browser-dynamic": "16.2.3",
    "@angular/router": "16.2.3",
    "jsgantt-improved": "*",
    "rxjs": "*",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.1",
    "@angular-devkit/schematics": "^16.2.1",
    "@angular/cli": "16.2.1",
    "@angular/compiler-cli": "16.2.3",
    "@angular/language-service": "16.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^13.7.1",
    "codelyzer": "^6.0.0",
    "cpr": "^3.0.0",
    "jasmine": "^3.3.0",
    "jasmine-core": "^5.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^16.2.2",
    "protractor": "~7.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.46.3",
    "tslib": "^2.6.1",
    "tslint": "~6.1.3",
    "typescript": "~5.1.6"
  }
}
